class Solution:
    def strStr(self, haystack: str, needle: str) -> int:
        l1,l2=len(haystack),len(needle)
        if needle=="":
            return 0

        for i in range(l1+1-l2):
            if haystack[i:len(needle)+i]==needle:
                return i
        return -1        


        

             

                   
           
               

        
